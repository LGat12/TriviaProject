<Window x:Class="TriviaProject.View.Frames"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TriviaProject.View"
        mc:Ignorable="d"

        Title="frames" Height="600" Width="900">
    

    <Grid>

        <Grid.Background>
            <ImageBrush ImageSource="C:\Users\User\source\repos\TriviaProject\TriviaProject\Images\QuizBackround.jpg"/>
        </Grid.Background>
        
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>
        <Menu Grid.Row="0">
            <MenuItem Header="Logout" Name="logoutMenu" Click="ClickLogout"/>
            <MenuItem Header="Exit" Name="exitMenu" Click="ClickClose"/>
        </Menu>
        <Frame x:Name="mainFrame" NavigationUIVisibility="Visible" Grid.Row="1"/>

    </Grid>
</Window>
